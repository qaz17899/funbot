# Environment Configuration
# Copy this file to .env and fill in the values

# Discord Bot Token (required)
DISCORD_TOKEN=your_discord_token_here

# Discord Client ID (optional, for OAuth)
DISCORD_CLIENT_ID=

# PostgreSQL Database URL (required)
# Format: postgresql://user:password@host:port/database
DB_URL=postgresql://postgres:password@localhost:5432/funbot

# Redis URL (optional, for caching)
# Format: redis://[[username]:[password]]@host:port/db
REDIS_URL=redis://localhost:6379/2

# Environment: dev, prod
ENV=dev

# Dev Guild ID (only sync commands to this guild in dev environment)
DEV_GUILD_ID=770976907163729940

# Logging level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=DEBUG
